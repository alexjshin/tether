{
  "name": "tether",
  "version": "1.0.0",
  "description": "A personal finance app for tracking income, expenses, budgets, and net worth with visual insights.",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev": "npm run dev:backend & npm run dev:frontend",
    "install:all": "npm install",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules"
  },
  "keywords": [
    "personal-finance",
    "budget",
    "expense-tracker"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.6"
  }
}
